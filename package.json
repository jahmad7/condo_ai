{
  "name": "next-firebase-saas-kit",
  "version": "0.17.4",
  "description": "",
  "private": true,
  "sideEffects": false,
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "scripts": {
    "analyze": "ANALYZE=true npm run build",
    "dev": "next dev",
    "build": "next build",
    "build:test": "NODE_ENV=test next build",
    "dev:test": "NODE_ENV=test next dev",
    "postbuild": "npm run sitemap && npm run rss",
    "rss": "npx tsx './src/core/blog/rss-feed.ts'",
    "sitemap": "next-sitemap",
    "start": "next start",
    "start:test": "NODE_ENV=test next start",
    "typecheck": "tsc",
    "lint": "next lint",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "healthcheck": "npm run lint && npm run typecheck",
    "firebase:emulators:start": "firebase emulators:start --project demo-makerkit --import ./firebase-seed",
    "firebase:emulators:export": "firebase emulators:export --project demo-makerkit ./firebase-seed",
    "cypress": "NODE_ENV=test cypress open",
    "cypress:headless": "NODE_ENV=test cypress run",
    "test": "firebase emulators:exec --project demo-makerkit --import ./firebase-seed \"sh ./scripts/test.sh build\"",
    "stripe:listen": "docker run --rm -it --name=stripe -v ~/.config/stripe:/root/.config/stripe stripe/stripe-cli:latest listen --forward-to http://host.docker.internal:3000/api/stripe/webhook",
    "killports": "sh scripts/kill-ports.sh",
    "inbucket:start": "docker run --rm -d -p 9000:9000 -p 2500:2500 -p 1100:1100 inbucket/inbucket",
    "inbucket:stop": "docker kill inbucket"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.1",
    "@mdx-js/mdx": "^3.0.1",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-popover": "1.0.7",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@react-email/components": "0.0.15",
    "@sentry/node": "^7.105.0",
    "@sentry/react": "^7.105.0",
    "@stripe/react-stripe-js": "^2.5.1",
    "@stripe/stripe-js": "^3.0.7",
    "@tanstack/react-table": "^8.13.2",
    "clsx": "^2.1.0",
    "csrf": "^3.1.0",
    "cva": "npm:class-variance-authority@^0.7.0",
    "date-fns": "3.3.1",
    "feed": "^4.2.2",
    "firebase": "^9.23.0",
    "firebase-admin": "^12.0.0",
    "heroicons": "2.1.1",
    "i18next": "23.10.0",
    "next": "14.1.3",
    "next-i18next": "^15.2.0",
    "nodemailer": "^6.9.11",
    "nookies": "^2.5.2",
    "pino": "^8.19.0",
    "raw-body": "^2.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.0",
    "react-i18next": "^14.0.8",
    "react-top-loading-bar": "^2.3.1",
    "reactfire": "^4.2.3",
    "sonner": "^1.4.3",
    "stripe": "^14.19.0",
    "swr": "^2.2.5",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^6.0.1",
    "@next/bundle-analyzer": "14.1.3",
    "@types/node": "^20.11.21",
    "@types/nodemailer": "^6.4.14",
    "@types/react": "18.2.64",
    "@types/react-dom": "^18.2.21",
    "autoprefixer": "^10.4.18",
    "cypress": "13.6.6",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.1.3",
    "firebase-tools": "^13.4.0",
    "gray-matter": "4.0.3",
    "next-sitemap": "^4.2.3",
    "pino-pretty": "10.3.1",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-sanitize": "^6.0.0",
    "rehype-slug": "^6.0.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.2"
  },
  "optionalDependencies": {
    "recharts": "^2.12.2"
  }
}